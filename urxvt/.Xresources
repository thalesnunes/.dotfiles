! Fonts config
URxvt.font: xft:MesloLGS NF:style=Regular:size=14
URxvt.boldFont: xft:MesloLGS NF:style=Bold:size=14
URxvt.italicFont: xft:MesloLGS NF:style=Italic:size=14
URxvt.boldItalicFont: xft:MesloLGS NF:style=Bold Italic:size=14
URxvt.urgentOnBell: false
URxvt.scrollBar: false
URxvt.intensityStyles: false
URxvt.termName: rxvt-unicode-256color
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.cursorBlink: false
URxvt.cursorUnderline: false
URxvt.scrollBar_right: false
URxvt.iso14755: false
URxvt.depth: 32

! Disable scrollbar
! Dracula Xresources palette
*.foreground: #c3c7d1
*.background: [90]#2a2844

! Black
*.color0:     #6b768b
*.color8:     #6b768b

! Red
*.color1:     #ed254e
*.color9:     #ed254e

! Green
*.color2:     #71f79f
*.color10:    #71f79f

! Yellow
*.color3:     #f9dc5c
*.color11:    #f9dc5c

! Blue
*.color4:     #7cb7ff
*.color12:    #7cb7ff

! Magenta
*.color5:     #c74ded
*.color13:    #c74ded

! Cyan
*.color6:     #00c1e4
*.color14:    #00c1e4

! White
*.color7:     #dcdfe4
*.color15:    #dcdfe4


!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
